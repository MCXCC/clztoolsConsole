<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.clztoolsconsole.material.mapper.MaterialMapper">
    <resultMap id="Material" type="org.example.clztoolsconsole.material.entity.Material">
        <id property="id" column="id"/>
        <result property="workshop" column="workshop"/>
        <result property="gang" column="gang"/>
        <result property="name" column="name"/>
        <result property="specification" column="specification"/>
        <result property="number" column="number"/>
        <result property="pictureId" column="picture_id"/>
        <result property="remark" column="remark"/>
        <result property="createdAt" column="created_at" javaType="java.util.Date"/>
        <result property="updatedAt" column="updated_at" javaType="java.util.Date"/>
        <result property="isDeleted" column="is_deleted"/>
        <result property="version" column="version"/>
        <result property="sortOrder" column="sort_order"/>
    </resultMap>
    <select id="findList" resultMap="Material">
        SELECT * FROM material
    </select>
</mapper>